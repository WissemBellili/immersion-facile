{
  "version": "0.0.0",
  "scripts": {
    "test": "npm run symlink-shared && jest",
    "dev": "npm run symlink-shared && npm run gen-env-config && vite",
    "dev-http": "npm run symlink-shared && GATEWAY=HTTP npm run gen-env-config && vite",
    "build": "npm run symlink-shared && vite build",
    "serve": "npm run prod-env-config && vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "gen-env-config": "chmod +x ./env.sh && ./env.sh",
    "prod-env-config": "npm run gen-env-config && cp env-config.js dist/env-config.js",
    "format:check": "prettier --check src",
    "fullcheck": "npm run format && npm run test && npm run typecheck",
    "symlink-shared": "ln -fs ../../back/src/shared ./src/"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.1.0",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "formik": "2.1.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "type-route": "^0.6.0",
    "uuid": "^8.3.2",
    "zod": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-react": "^1.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  }
}
